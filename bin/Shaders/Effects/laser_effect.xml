<xml>
  <Effect >
    <Technique name="default">
      <VertexShader entry_point="vs_main" file_name="Shaders\Shaders\laser_shader.fx" />
      <PixelShader entry_point="ps_main" file_name="Shaders\Shaders\laser_shader.fx" />
      <BlendState  alphablend="1"/>
      <RasterState Cullmode="3" FillMode="3" FrontCCW="1" />
      <DepthStencilState depth_enabled="1" stencil_enabled="0" />
      <VertexDeclarationDescriptor position_dimension="3" normal="false" tangent="false" vertex_color="false" textureCoordsDimensions="" />
      <PrimitiveType type="3" />
    </Technique>
    <Technique name="shadow_map">
      <VertexShader entry_point="vs_main" file_name="Shaders\Shaders\laser_shadow_shader.fx" />
      <PixelShader entry_point="ps_main" file_name="Shaders\Shaders\laser_shadow_shader.fx" />
      <BlendState  alphablend="0"/>
      <RasterState Cullmode="3" FillMode="3" FrontCCW="1" />
      <DepthStencilState depth_enabled="1" stencil_enabled="0" />
      <VertexDeclarationDescriptor position_dimension="3" normal="false" tangent="false" vertex_color="false" textureCoordsDimensions="" />
      <PrimitiveType type="3" />
    </Technique>
    <!--
    <Technique name="cube_map">
      <VertexShader entry_point="vs_main_cube" file_name="Shaders\laser_shader.fx" />
      <GeometryShader entry_point="gs_main" file_name="Shaders\laser_shader.fx" />
      <PixelShader entry_point="ps_main_cube" file_name="Shaders\laser_shader.fx" />
    </Technique>
    -->
  </Effect>
</xml>